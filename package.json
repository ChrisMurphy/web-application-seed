{
  "name": "webapplicationseed",
  "version": "1.0.0",
  "description": "StepChange Angular2 Application Seed",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "clean": "node_modules/.bin/rimraf build",
    "build": "npm run tsc && npm run tslint",
    "pretest": "npm run clean && npm run build",
    "postinstall": "npm run typings",
    "test": "node_modules/.bin/karma start karma.conf.js",
    "serve": "lite-server",
    "typings": "node_modules/.bin/typings install",
    "watch": "tsc --watch",
    "tsc": "node_modules/.bin/tsc --outDir build",
    "tslint": "node_modules/.bin/tslint -c tslint.json app/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "http://fcctfs:8080/tfs/tfssandpit/FrontEndApplications/_git/WebApplicationSeed"
  },
  "author": "StepChange",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.29",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "jasmine": "2.3.2",
    "karma": "^0.13.22",
    "lite-server": "^2.2.0",
    "rimraf": "^2.5.2",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.1.0"
  }
}
