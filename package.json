{
  "name": "webapplicationseed",
  "version": "1.0.0",
  "description": "StepChange Angular2 Application Seed",
  "main": "index.html",
  "scripts": {
    "clean-build": "rimraf dist",
    "clean-dependencies": "rimraf node_modules typings",
    "clean-install": "npm run clean-dependencies && npm install",
    "install-globals": "npm install rimraf protractor webdriver-manager -g",
    "postinstall": "npm run install-globals && npm run typings",
    "test": "karma start config/karma.conf.js",
    "typings": "typings install",
    "protractor": "protractor config/protractor.conf.js",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "build": "npm run clean-build && webpack --config config/webpack.prod.js",
    "start": "webpack-dev-server --config config/webpack.dev.js --progress --colors --watch --inline --display-error-details",
    "start:prod": "webpack-dev-server --config config/webpack.prod.js --progress --colors --watch --inline"
  },
  "repository": {
    "type": "git",
    "url": "http://fcctfs:8080/tfs/tfssandpit/FrontEndApplications/_git/WebApplicationSeed"
  },
  "author": "StepChange",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "^0.19.29",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.23",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "jasmine": "2.3.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.9.1",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
